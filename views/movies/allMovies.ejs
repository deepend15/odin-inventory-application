<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include('../partials/favicon') %>
    <title><%= title %></title>
  </head>
  <body>
    <%- include('../partials/nav') %>
    <h1><%= title %></h1>
    <a href="/movies/add-movie"><button>+ Add movie</button></a>
    <ul>
      <% movies.forEach((movie) => { %>
        <li>
          <a href="movies/<%= movie.url_path %>"><%= movie.title %></a>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
