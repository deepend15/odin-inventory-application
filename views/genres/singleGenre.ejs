<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include('../favicon') %>
    <title><%= title %></title>
  </head>
  <body>
    <%- include('../nav') %>
    <h1>Genre > <%= title %></h1>
    <ul>
      <% movies.forEach((movie) => { %>
        <li>
          <a href="/movies/<%= movie.url_path %>"><%= movie.title %></a>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
