<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include('../partials/favicon') %>
    <title><%= title %></title>
  </head>
  <body>
    <%- include('../partials/nav') %>
    <h1>Studios > <%= title %></h1>
    <% if (movies.length > 0) { %>
    <ul>
      <% movies.forEach((movie) => { %>
        <li>
          <a href="/movies/<%= movie.url_path %>"><%= movie.title %></a>
        </li>
      <% }); %>
    </ul>
    <% } else { %>
      <p>No movies in stock for this studio.</p>
    <% } %>
    <a href="/studios">&lt;&lt; All studios</a>
  </body>
</html>
